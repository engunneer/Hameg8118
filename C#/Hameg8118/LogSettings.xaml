<Window x:Class="Hameg8118.LogSettings"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Hameg8118"
        mc:Ignorable="d"
        Title="Logging Settings" Height="130" Width="290" ResizeMode="NoResize" Icon="icon.ico" KeyUp="Window_KeyUp">
    <Grid>
        <TextBox x:Name="textBoxInterval" HorizontalAlignment="Left" Height="23" Margin="110,10,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="75" VerticalContentAlignment="Center"/>
        <Label x:Name="labelInterval" Content="Logging interval" HorizontalAlignment="Left" Margin="10,7,0,0" VerticalAlignment="Top"/>
        <ComboBox x:Name="comboBoxUnit" HorizontalAlignment="Left" Margin="190,10,0,0" VerticalAlignment="Top" Width="75"/>
        <Button x:Name="buttonOK" Content="OK" HorizontalAlignment="Left" Margin="110,63,0,0" VerticalAlignment="Top" Width="75" Click="buttonOK_Click"/>
        <Button x:Name="buttonCancel" Content="Cancel" HorizontalAlignment="Left" Margin="190,63,0,0" VerticalAlignment="Top" Width="75" Click="buttonCancel_Click"/>
        <CheckBox x:Name="checkBoxMilliseconds" Content="Include milliseconds in timestamp" HorizontalAlignment="Left" Margin="15,41,0,0" VerticalAlignment="Top"/>
    </Grid>
</Window>
